<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.origin.ueliton.bulltrail">

    <application
        android:name=".BullTrail"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Animals.AnimalsActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AnimalDetail.AnimalDetailActivity" />
        <activity
            android:name=".activity.ModelActivity"
            android:label="@string/title_activity_model"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
